                            "x-order": 3
                          },
                          "unit": {
                            "description": "ParamUnit represents template parameter unit.",
                            "type": "string",
                            "default": "PARAM_UNIT_INVALID",
                            "enum": [
                              "PARAM_UNIT_INVALID",
                              "PERCENTAGE"
                            ],
                            "x-order": 2
                          }
                        }
