          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "enabled": {
                  "description": "BooleanFlag represent a command to enable some boolean property (set to true),\ndisable some boolean property (set to false), or avoid changing that property.\n\n - DO_NOT_CHANGE: Do not change boolean property. Default value.\n - ENABLE: Enable boolean property.\n - DISABLE: Disable boolean property.",
                  "type": "string",
                  "default": "DO_NOT_CHANGE",
                  "enum": [
                    "DO_NOT_CHANGE",
                    "ENABLE",
                    "DISABLE"
                  ],
                  "x-order": 1
                },
                "enabled_does_not_work": {
                  "description": "New rule status.\nGo Swagger code generates bool :(\nFIXME Remove.",
                  "type": "boolean",
                  "x-order": 2
                },
                "for": {
                  "type": "string",
                  "title": "Rule set duration. Zero value will not change it.\nFIXME ?",
                  "x-order": 4
                },
                "name": {
                  "description": "Rule name.",
                  "type": "string",
                  "x-order": 0
                },
                "params": {
                  "description": "Parameters to change. Missing parameters will not be changed.",
