      "get": {
        "description": "List all dumps",
        "tags": [
          "DumpService"
        ],
        "summary": "List All Dumps",
        "operationId": "ListDumps",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "type": "object",
              "properties": {
                "dumps": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "created_at": {
                        "type": "string",
                        "format": "date-time",
                        "x-order": 5
                      },
                      "dump_id": {
                        "type": "string",
                        "x-order": 0
                      },
                      "end_time": {
                        "type": "string",
                        "format": "date-time",
                        "x-order": 4
                      },
                      "service_names": {
                        "type": "array",
                        "items": {
                          "type": "string"
                        },
                        "x-order": 2
                      },
                      "start_time": {
                        "type": "string",
                        "format": "date-time",
                        "x-order": 3
                      },
                      "status": {
                        "type": "string",
                        "default": "DUMP_STATUS_UNSPECIFIED",
                        "enum": [
                          "DUMP_STATUS_UNSPECIFIED",
                          "DUMP_STATUS_IN_PROGRESS",
                          "DUMP_STATUS_SUCCESS",
                          "DUMP_STATUS_ERROR"
                        ],
                        "x-order": 1
                      }
                    }
                  },
                  "x-order": 0
                }
              }
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "type": "object",
              "properties": {
                "code": {
                  "type": "integer",
                  "format": "int32",
                  "x-order": 0
                },
                "details": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "@type": {
                        "type": "string",
                        "x-order": 0
                      }
                    },
                    "additionalProperties": false
                  },
                  "x-order": 2
                },
                "message": {
                  "type": "string",
                  "x-order": 1
                }
              }
            }
          }
        }
      }
